{
	"name": "limit-order-dapp",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"env:start": "../scripts/local/start.sh",
		"env:stop": "../scripts/local/stop.sh",
		"dev:local": "npm run env:start && vite dev",
		"test": "playwright test",
		"test:headed": "playwright test --headed",
		"test:ui": "playwright test --ui",
		"test:debug": "playwright test --debug",
		"test:report": "playwright show-report",
		"test:fix": "playwright test --update-snapshots",
		"setup-and-test": "npm run env:stop && rm -rf node_modules/.vite && rm -rf .svelte-kit && npm run prepare && npm run env:start && npm run test",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && npm run generate:types",
		"check": "svelte-kit sync && svelte-check",
		"check:watch": "svelte-kit sync && svelte-check --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"generate:types": "tsx scripts/generate-contracts.ts",
		"generate:fixtures": "tsx -r tsconfig-paths/register fixtures/index.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.7",
		"@typechain/ethers-v5": "^11.1.2",
		"@types/node": "^22.15.21",
		"abitype": "^1.0.8",
		"chalk": "^5.4.1",
		"ethers": "^5.8.0",
		"flowbite-svelte": "^1.4.3",
		"flowbite-svelte-icons": "^2.2.0",
		"lucide-svelte": "^0.511.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.33.1",
		"svelte-5-ui-lib": "^0.12.6",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.7",
		"tsx": "^4.19.4",
		"typechain": "^8.3.2",
		"typescript": "^5.8.3",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@reown/appkit": "^1.7.6",
		"@reown/appkit-adapter-wagmi": "^1.7.6",
		"@tanstack/react-query": "^5.77.0",
		"@uniswap/sdk-core": "^7.7.2",
		"@uniswap/v4-sdk": "^1.21.4",
		"graphql": "^16.11.0",
		"graphql-request": "^7.1.2",
		"viem": "^2.30.1",
		"wagmi": "^2.15.4"
	}
}
